{
  "name": "ssh-exec",
  "version": "2.5.0",
  "description": "Execute a script over ssh using Node.JS",
  "main": "index",
  "scripts": {
    "lint": "standard",
    "test:unit": "tap -J --comments --no-esm --strict test/*.test.js",
    "test:unit:debug": "tap -T --node-arg=--inspect-brk --comments --no-esm --strict test/*.test.js",
    "test:coverage": "npm run test:unit -- --cov --coverage-report=html",
    "test": "npm run lint && npm run test:unit"
  },
  "dependencies": {
    "duplexify": "^4.1.1",
    "once": "^1.4.0",
    "ssh2": "^0.8.9"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "standard": "^14.3.4",
    "tap": "^14.10.7"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=8.17.0"
  },
  "homepage": "https://github.com/mafintosh/ssh-exec",
  "repository": {
    "type": "git",
    "url": "git://github.com/mafintosh/ssh-exec.git"
  },
  "bugs": {
    "url": "https://github.com/mafintosh/ssh-exec/issues"
  },
  "keywords": [
    "ssh",
    "connection",
    "duplex",
    "exec",
    "stream"
  ],
  "author": "Mathias Buus (@mafintosh)",
  "license": "MIT"
}
